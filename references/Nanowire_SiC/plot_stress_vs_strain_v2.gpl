#!/usr/bin/gnuplot

file_name = "stress_vs_strain_v2.txt"

set terminal png
set out "stress_vs_strain.png"

set xlabel "Strain / %"
set xtics 0.1
set mxtics 5
set ylabel "Stress / GPa"
set mytics 5

set title "Stress vs. Strain"

plot file_name u ($2/1):4 w l t ""

set terminal win font "Arial,12"
replot
